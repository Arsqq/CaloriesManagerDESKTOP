<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="287.0" prefWidth="525.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kursach.OOPProject.Controllers.CaloriesNormController">
   <children>
      <HBox prefHeight="108.0" prefWidth="524.0">
         <children>
            <Button fx:id="breakfastButton" mnemonicParsing="false" onAction="#getMorningCombination" prefHeight="108.0" prefWidth="525.0" style="-fx-background-color: #808080;" text="Breakfast">
               <font>
                  <Font name="Rustico Regular" size="34.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <HBox layoutY="108.0" prefHeight="100.0" prefWidth="525.0">
         <children>
            <Button fx:id="lunchButton" mnemonicParsing="false" prefHeight="128.0" prefWidth="525.0" style="-fx-background-color: #a63232;" text="Lunch">
               <font>
                  <Font name="Rustico Regular" size="34.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <HBox layoutY="208.0" prefHeight="100.0" prefWidth="525.0">
         <children>
            <Button fx:id="dinnerButton" mnemonicParsing="false" prefHeight="113.0" prefWidth="552.0" style="-fx-background-color: #5e67b8;" text="Dinner">
               <font>
                  <Font name="Rustico Regular" size="34.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
